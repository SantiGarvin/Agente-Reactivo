{
	"version": "0.2.0",
	"configurations": [
		 {
			  "name": "Debug cmake project",
			  "type": "cppdbg",
			  "request": "launch",
			  "program": "${workspaceFolder}/build/practica1",
			  "args": ["${workspaceFolder}/mapas/mapa30.map", "0", "0", "3", "3", "3"],
			  "stopAtEntry": false,
			  "cwd": "${workspaceFolder}",
			  "environment": [],
			  "externalConsole": false,
			  "MIMode": "gdb",
			  "miDebuggerPath": "/usr/bin/gdb",
			  "preLaunchTask": "Build cmake project",
			  "setupCommands": [
					{
						 "description": "Enable pretty-printing for gdb",
						 "text": "-enable-pretty-printing",
						 "ignoreFailures": true
					}
			  ]
		 }
	]
}
